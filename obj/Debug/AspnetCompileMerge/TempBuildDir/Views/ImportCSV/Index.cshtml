@using CSCLite.Models
@model IEnumerable<ImportModel>
@{
    ViewBag.Title = "Import CSV";
    ViewBag.Description = "นำเข้าเอกสาร";
}
<div class="row">
    <div class="col-xs-12 col-md-5">
        <div class="box box-default">
            <div class="box box-primary box-solid">
                <div class="box-header with-border box-primary">
                    <h3 class="box-title">DTL </h3>

                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
                    </div>
                    <!-- /.box-tools -->
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    @using (Html.BeginForm("Index", "ImportCSV", FormMethod.Post, new { enctype = "multipart/form-data" }))
                    {


                    
                        <!-- /.box-header -->
                        <!-- form start -->
                        <form class="form-horizontal">
                            <div class="progress">
                                <div class="progress-bar progress-bar-striped bg-success" role="progressbar" style="width: 25%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                            </div>
                            <div class="box-body">



                                <div class="form-group">
                                    <label for="" class="col-sm-2 control-label">File  Name</label>

                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" id="inputPassword3" placeholder="Password">
                                    </div>
                                </div>

                            </div>
                            <!-- /.box-body -->

                            <div class="box-footer">

                                <div class="row">

                                    <div class="col-sm-2 pull-right">
                                        <input type="submit" value="Import" class="btn btn-info pull-right">
                                    </div>
                                    <div class="col-sm-2 pull-right">
                                        <span class="btn btn-info btn-file pull-right">
                                            Browse   <input type="file" name="postedFile" value="postedFile" class="btn btn-default">
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <!-- /.box-footer -->
                        </form>
                    }
                </div>
                <!-- /.box-body -->
            </div>
        </div>
    </div>

    <div class="col-xs-12 col-md-5">
        <div class="box box-default">
